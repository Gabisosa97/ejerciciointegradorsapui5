<mvc:View
    controllerName="acc.fcl.ejerciciointegradorfcl.controller.App"
    displayBlock="true"
    xmlns="sap.f"
    xmlns:m="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
>
    <m:App id="app">
        <FlexibleColumnLayout
            stateChange="onStateChanged"
            id="fcl"
            backgroundDesign="Solid"
            layout="{appView>/layout}"
        >
            <beginColumnPages>
                <mvc:XMLView
                    id="beginView"
                    viewName="acc.fcl.ejerciciointegradorfcl.view.List"
                />
            </beginColumnPages>
            <midColumnPages>
                <mvc:XMLView
                    id="detailView"
                    viewName="acc.fcl.ejerciciointegradorfcl.view.Detail"
                />
            </midColumnPages>
        </FlexibleColumnLayout>
    </m:App>
</mvc:View>
